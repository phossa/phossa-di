{
    "name": "phossa/phossa-di",
    "license": "MIT",
    "type": "library",
    "description": "A fast and full-fledged dependency injection library for PHP. It supports auto wiring, container delegation, object decorating, definition provider, definition tags, service scope and more.",
    "keywords": ["phossa", "dependency injection", "container"],
    "homepage": "https://github.com/phossa/phossa-di",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa"
        }
    ],
    "autoload": {
        "psr-0": { "Phossa\\Di\\": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Phossa\\Di\\": "tests/src/" }
    },
    "require": {
        "php": ">=5.4.0",
        "phossa/phossa-shared": ">=1.0.6",
        "phossa/phossa-config": "dev-master",
        "container-interop/container-interop": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "suggest": {
        "phossa/phossa-cache": "If you want to use config cache"
    },
    "archive": {
        "exclude": ["!vendor", "tests", "*phpunit.xml",  "composer.lock"]
    }
}
